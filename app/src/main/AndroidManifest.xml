<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="design.material.nainesh.materialdesigncomponents">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ButtonsActivity"/>

        <activity android:name=".activity.BottomSheetsActivity"/>
        <activity android:name=".activity.CardViewActivity"/>
        <activity android:name=".activity.MenuActivity"/>
        <activity android:name=".activity.ProgressBarActivity"/>
        <activity android:name=".activity.RatingBarActivity"/>
        <activity android:name=".activity.SelectionControlActivity"/>
        <activity android:name=".activity.SnackBarActivity"/>
        <activity android:name=".activity.CoOrdinateLayoutActivity"/>

    </application>

</manifest>